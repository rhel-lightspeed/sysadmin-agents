# Example secrets configuration
# Copy this file and fill in your actual values
# DO NOT commit the actual secrets file to version control

---
# Gemini API Key
apiVersion: v1
kind: Secret
metadata:
  name: gemini-api-key
  namespace: adk-web
type: Opaque
stringData:
  GOOGLE_API_KEY: "your-gemini-api-key-here"

---
# SSH Private Key for linux-mcp-server
apiVersion: v1
kind: Secret
metadata:
  name: ssh-private-key
  namespace: adk-web
type: Opaque
stringData:
  id_ed25519: |
    -----BEGIN OPENSSH PRIVATE KEY-----
    YOUR_SSH_PRIVATE_KEY_CONTENT_HERE
    -----END OPENSSH PRIVATE KEY-----

---
# SSH Credentials (optional)
apiVersion: v1
kind: Secret
metadata:
  name: ssh-credentials
  namespace: adk-web
type: Opaque
stringData:
  username: "your-ssh-username"

